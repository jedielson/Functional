<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Functional</title>
    @Styles.Render("~/Content/css")
    @Styles.Render("~/Content/bootstrap-tree.css")
    @Styles.Render("~/Content/chosen")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="navbar navbar-inverse navbar-fixed-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle" data-toggle="collapse" data-target=".navbar-collapse">
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                @Html.ActionLink("Functional", "Index", "Home", null, new { @class = "navbar-brand" })
                @Html.ActionLink("Requisitos", "Index", "Requisito", null, new { @class = "navbar-brand" })
                @Html.ActionLink("Casos de Uso", "Index", "CasoDeUso", null, new { @class = "navbar-brand" })
            </div>
        </div>
    </div>
    <div class="container body-content">
        @RenderBody()
        <hr />

    </div>
    <div class="container navbar navbar-fixed-bottom">
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - Functional</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/bootstrap")
    @Scripts.Render("~/bundles/Functional")
    @Scripts.Render("~/Scripts/chosen")
    @RenderSection("scripts", required: false)
    <script type="text/javascript" src="~/Scripts/Controller/ProjetoSeletorScripts.js"></script>
</body>
</html>
